
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm())
{
    
    

    <input type="submit" name="Add" value="Add" />    <br />
    <input type="submit" name="Add" value="Upload" /> <br />
}
<div class="postcontainer" padding="30">

    <div class="feed">

       
        @foreach(var post in Model.PostList)
        {
            <div class="feed">
                <p>@post.Title</p>
                <p>@post.Content</p>
                <p>@post.CreatedAt</p>
            </div>
        }

    </div>
